#logic               ;;


type nat = Zero | S of nat


let rec add a0 a1 = match (a0, a1) with
  | (Zero, y) -> y
  | ((S(x)), y) -> (S((add x y)))

let rec leq a0 a1 = match (a0, a1) with
  | (Zero, y) -> (not false)
  | ((S(x)), Zero) -> false
  | ((S(x)), (S(y))) -> (leq x y)



;;

verify(        fun v0 v1 v2 v3 v4 v5 -> ((add (S((S(v4)))) (add v1 (add (add (add (S((S((add (S(v5)) v1))))) (S(v0))) v2) (add (S(v2)) v3)))) = (S((add v5 (add (add v2 (add v0 v2)) (add (add (S(v1)) v4) (add (S((S((S((S((S(v1)))))))))) v3)))))))             ) [@@auto]
