#logic               ;;


type nat = Zero | S of nat


let rec add a0 a1 = match (a0, a1) with
  | (Zero, y) -> y
  | ((S(x)), y) -> (S((add x y)))

let rec leq a0 a1 = match (a0, a1) with
  | (Zero, y) -> (not false)
  | ((S(x)), Zero) -> false
  | ((S(x)), (S(y))) -> (leq x y)



;;

verify(        fun v0 v1 v2 v3 v4 v5 -> ((add (S((add (S(v0)) (add v5 (S(v3)))))) (S((S((S((add (S(v4)) (add (S(v2)) (S(v1))))))))))) = (add (add (S((add (S((S(v3)))) (add (S(v1)) v4)))) (add (S((S((S(v2)))))) v0)) (S((S(v5))))))             ) [@@auto]
