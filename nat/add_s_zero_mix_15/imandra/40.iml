#logic               ;;


type nat = Zero | S of nat


let rec add a0 a1 = match (a0, a1) with
  | (Zero, y) -> y
  | ((S(x)), y) -> (S((add x y)))

let rec leq a0 a1 = match (a0, a1) with
  | (Zero, y) -> (not false)
  | ((S(x)), Zero) -> false
  | ((S(x)), (S(y))) -> (leq x y)



;;

verify(        fun v0 v1 v2 v3 v4 v5 -> ((add (add (S(v5)) (add (add (S(v2)) v0) (S((S(v1)))))) (S((S((add (add v0 (S(v3))) (S(v4)))))))) = (add (add (S((add (add v1 (S((S((S((add (S((S(v5)))) v4)))))))) (S(v0))))) (S(v3))) (add v0 v2)))             ) [@@auto]
