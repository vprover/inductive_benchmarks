#logic               ;;


type nat = Zero | S of nat


let rec add a0 a1 = match (a0, a1) with
  | (Zero, y) -> y
  | ((S(x)), y) -> (S((add x y)))

let rec leq a0 a1 = match (a0, a1) with
  | (Zero, y) -> (not false)
  | ((S(x)), Zero) -> false
  | ((S(x)), (S(y))) -> (leq x y)



;;

verify(        fun v0 v1 v2 -> ((S((add (add (S(v1)) Zero) (add (S(v0)) (add v0 (S(v2))))))) = (add (add (add v1 v0) (S((S((S((S(v2))))))))) (add Zero v0)))             ) [@@auto]
