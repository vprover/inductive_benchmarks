     (DEFUN S (X0) (LIST X0))
     (DEFUN ADD (X0 X1)
            (COND ((ENDP X0) X1)
                  ((CONSP X0) (S (ADD (CAR X0) X1)))))
     (DEFUN LEQ (X0 X1)
            (COND ((ENDP X0) T)
                  ((AND (CONSP X0) (ENDP X1)) NIL)
                  ((AND (CONSP X0) (CONSP X1))
                   (LEQ (CAR X0) (CAR X1)))))
     (DEFTHM THEOREM
             (= (ADD (S V3)
                     (S (ADD (ADD V0 (ADD (S (S V1)) (S V3)))
                             (ADD (ADD V2 (S V2)) (ADD (S V5) V4)))))
                (ADD (S V1)
                     (ADD (ADD (ADD V5 (S (S (ADD V4 (S (ADD V3 V3))))))
                               (S V2))
                          (S (S (ADD V2 V0))))))))
