     (DEFUN S (X0) (LIST X0))
     (DEFUN ADD (X0 X1)
            (COND ((ENDP X0) X1)
                  ((CONSP X0) (S (ADD (CAR X0) X1)))))
     (DEFUN LEQ (X0 X1)
            (COND ((ENDP X0) T)
                  ((AND (CONSP X0) (ENDP X1)) NIL)
                  ((AND (CONSP X0) (CONSP X1))
                   (LEQ (CAR X0) (CAR X1)))))
     (DEFTHM THEOREM
             (= (ADD (ADD (ADD (ADD V1 (ADD (S V1) V2)) (S V0))
                          (S V5))
                     (ADD (ADD V3 (S (S V4))) (S (S V6))))
                (ADD (ADD (S V1)
                          (ADD (S (S V0))
                               (ADD (ADD (S (S V3)) (S (S V6)))
                                    (ADD V2 (ADD V1 V4)))))
                     V5))))
