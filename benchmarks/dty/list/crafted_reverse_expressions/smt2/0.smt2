(set-logic UFDT)
(declare-datatypes ((nat 0) (lst 0)) (((zero) (s (s0 nat))) ((nil) (cons (cons0 nat) (cons1 lst)))))
(declare-fun app (lst lst) lst)
(non-erasing app 2)
(injective app (true true))
(declare-fun rev (lst) lst)
(declare-fun revAcc (lst) lst)
(declare-fun revAccInner (lst lst) lst)
(assert (forall ((r lst)) (= (app nil r) r)))
(assert (forall ((a nat) (l lst) (r lst)) (= (app (cons a l) r) (cons a (app l r)))))
(assert (= (rev nil) nil))
(assert (forall ((x nat) (xs lst)) (= (rev (cons x xs)) (app (rev xs) (cons x nil)))))
(assert (forall ((x lst)) (= (revAcc x) (revAccInner x nil))))
(assert (forall ((acc lst)) (= (revAccInner nil acc) acc)))
(assert (forall ((acc lst) (x nat) (xs lst)) (= (revAccInner (cons x xs) acc) (revAccInner xs (cons x acc)))))
(assert (not (forall ((x lst)) (= (revAcc x) (rev x)))))
(check-sat)
