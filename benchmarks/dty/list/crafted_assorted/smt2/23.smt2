(set-info :smt-lib-version 2.6)
(set-logic UFDT)
(set-info :category "crafted")
(declare-datatypes ((nat 0) (lst 0)) (((s (s0 nat)) (zero)) ((nil) (cons (cons0 nat) (cons1 lst)))))
(declare-fun interleave (lst lst) lst)
(assert (forall ((x lst)) (= (interleave nil x) x)))
(assert (forall ((x nat) (y lst) (z lst)) (= (interleave (cons x y) z) (cons x (interleave z y)))))
(declare-fun evens (lst) lst)
(declare-fun odds (lst) lst)
(assert (= (evens nil) nil))
(assert (forall ((x nat) (y lst)) (= (evens (cons x y)) (cons x (odds y)))))
(assert (= (odds nil) nil))
(assert (forall ((x nat) (y lst)) (= (odds (cons x y)) (evens y))))
(assert-not (forall ((x lst)) (= (interleave (evens x) (odds x)) x)))
