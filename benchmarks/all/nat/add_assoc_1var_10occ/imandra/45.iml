verify(        fun v0 -> ((add (add v0 (add (add v0 (add v0 (add v0 v0))) v0)) (add (add v0 v0) (add v0 v0))) = (add (add v0 (add v0 v0)) (add (add v0 (add v0 v0)) (add (add v0 (add v0 v0)) v0))))             ) [@@auto]
