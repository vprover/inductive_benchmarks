#logic               ;;


type nat = Zero | S of nat


let rec add a0 a1 = match (a0, a1) with
  | (Zero, y) -> y
  | ((S(x)), y) -> (S((add x y)))

let rec leq a0 a1 = match (a0, a1) with
  | (Zero, y) -> (not false)
  | ((S(x)), Zero) -> false
  | ((S(x)), (S(y))) -> (leq x y)



;;

verify(        fun v0 v1 -> ((S((add (S((add (S(Zero)) (S(v1))))) (S((S(v0))))))) = (S((add (S((S(v0)))) (add (S((S((S(v1)))))) Zero)))))             ) [@@auto]
